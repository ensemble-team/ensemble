<%if @project.branches.any? %>
  <% @project.branches.each do |branch| %>
    <%if branch.requests.any? %>
      <% branch.requests.each do |request| %>
        <%if request.id != nil && request.status == "In progress" %>
          <% user = User.find(request.collab_id) %>
          <%= user.username %>
          <%= request.message %>
          <%= button_to 'View Mix Request', request_path(request), method: :get%><br>
        <% else %>
          No current requests
        <% end %>
      <% end %>
    <% end %>
  <% end %>
<% end %>
